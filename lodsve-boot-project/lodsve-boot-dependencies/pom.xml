                <version>${lodsve.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>com.lodsve.boot</groupId>
                <artifactId>lodsve-boot-starter-mybatis</artifactId>
                <version>${lodsve.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>com.lodsve.boot</groupId>
                <artifactId>lodsve-boot-starter-nacos</artifactId>
                <version>${lodsve.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>com.lodsve.boot</groupId>
